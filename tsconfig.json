{
    "compilerOptions": {
        "outDir": "./build/",
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react-jsx",
        "allowJs": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "paths": {
            "*": [
                "./src/*"
            ]
        }
    }
}